$bass: note("<d a a2 [f2@4 f]>")
.s("piano")
.fast(2)
.struct("{x x@2 x}%4")
.clip(1)
// .att(.1)
// .decay(.2)
// .sus(.2)
// .pan(rand)
// .room(1)
// .roomsize(9)
.distort("4:.1")
.gain(1)
._scope()


// v2
$bass: note("<d a a2 [f2@4 f] d a2 [f2@4 f*2, d2*2], <[a4 a5 a4@4 d5*4 - - - - ]>>".sub(12))
.s("piano")
.struct("{x x@2 x ~ x!4 ~ ~ x*2 x@4}%8")
.clip(.85)
.att(.1)
.decay(.1)
.sus(.5)
.pan(rand)
// .lpf(100)
.room(1)
.roomsize(9)
.distort("4:.3")
.gain(1)
._punchcard()
